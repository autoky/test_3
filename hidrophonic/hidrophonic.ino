#include "Servo.h"
#include "Nutrisi.h"
#include "LED.h"
#include "ULTRASONIC.h"
#include "Serial_Read.h"

int data;

void setup() {
  // put your setup code here, to run once:
siservo.attach(3);
  
pinMode(NTRS, INPUT);
pinMode(KLMB, INPUT);
pinMode(LEDM, OUTPUT);
pinMode(LEDH, OUTPUT);
pinMode(ECHO, INPUT);
pinMode(TRIG, OUTPUT);

Serial.begin(9600);

}

void loop() {
  // put your main code here, to run repeatedly:
Nutrisi();
Kelembapan();
ULTRASONIC();
Serial_Read();

 delay(100);
}

